{
  "name": "node-server",
  "version": "1.0.0",
  "type": "module",
  "description": "Node.js Express proxy server for Python PDF API",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "start:server": "node dist/server.js",
    "dev": "nodemon --exec \"ts-node --esm src/index.ts\"",
    "dev:server": "nodemon --exec \"ts-node --esm src/server.ts\"",
    "dev:verbose": "nodemon --verbose --exec \"ts-node --esm src/index.ts\"",
    "dev:debug": "nodemon --inspect --exec \"ts-node --esm src/index.ts\"",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "test": "echo \"No tests specified\" && exit 0"
  },
  "keywords": [
    "express",
    "proxy",
    "pdf",
    "api"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "node-fetch": "^2.7.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "typescript": "^5.2.2",
    "@types/node": "^20.5.0",
    "@types/express": "^4.17.17",
    "@types/cors": "^2.8.13",
    "@types/node-fetch": "^2.6.4",
    "ts-node": "^10.9.1",
    "rimraf": "^5.0.1"
  }
}
